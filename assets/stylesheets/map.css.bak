/* CSS Document */
body { min-height:100vh; }

#map_tool .boxcol { border:1px solid #CCC; padding:20px; margin:0; }
#map_tool .infoBox { border:1px solid #67A1B7; padding:20px; margin:10px 0; display:none; }
#map_tool .infoBox h2, .boxcol h2 { font-size:20px; margin:0; padding:0; border-bottom:1px solid #CCC; padding-bottom:10px; }
#map_tool .infoBox h2 { border-color:#67A1B7; color:#67A1B7; }


#map_tool #blocElements.active { border-color:#67A1B7; }
#map_tool #blocElements.active h2 { color:#67A1B7; }

#map_tool .job.active { border-color:#67A1B7; }
#map_tool .job.active h3 { color:#67A1B7; }

#infoHelp, #infoExport { border-color:#CCC; }
#infoJobs { border-color:#CCC; }

#infoJobs h2 { margin-bottom:10px; color:#333; border-color:#CCC; }
#infoExport h2 { margin-bottom:10px; color:#333; border-color:#CCC; }

#map_tool .btn img { max-height:20px; }
#map_tool .btn img.blanc { display:none; }
#map_tool .btn img.noir { display:block; }
#map_tool .btn.btn-primary img.blanc { display:block; }
#map_tool .btn.btn-primary img.noir { display:none; }
#map_tool .btn-group .btn { font-size:15px; }
#map_tool #modalHelp .btn { font-size:15px; }
#map_tool .btn-help { display:inline-block; min-width:80px; }
#map_tool .ligne-help { margin-bottom:10px; }

.cone, .cone_dir, .route, .rayon, .robot, .robot_secure, .poly , .text, .route_rayon { z-index:500; }
.movable, .route_elem_num { display:none; }

#colonneInfos, #colonneElements { max-height:90vh; overflow:auto; }

.rayon { z-index:1000; }

.text.text_elem, .route_elem_num {  dominant-baseline:middle; text-anchor:middle; cursor:default; }

body .poly, body.addPoly .poly_elem_current {  fill:rgba(204,102,0, 0.5); stroke:#00CCFF; display:block; }
body .text, body.no_current .text.movable {  display:block !important; }
body .info, body.no_current .info.movable {  display:block !important; }

body .rayon, body.show_rayon .rayon { cursor:pointer; stroke:#00CCFF; display:block; }
.rayon.active, .rayon.active { cursor:pointer; stroke: red; display:block; }

.activ_over { stroke: black !important; fill: rgba(0,0,0,0.5) !important; visibility:visible; }
.activ_select { stroke: red !important; fill: rgba(255,0,0,0.5) !important; visibility:visible; }

text.activ_over { stroke: none !important; fill: red !important; text-decoration:underline; }
text.activ_select { stroke: none !important; fill: red !important; text-decoration:underline; }

text.active { text-decoration:underline; }

body .rayon.movable, body.show_rayon .rayon.movable { display:none; }
.rayon.active.movable, .rayon.active.movable { stroke: transparent; fill:rgba(0,0,0, 0.8); display:block; }

body .route, body .route.route_elem_0, body.show_route .route, .route.active, body.select_path .route { stroke:#00FF00; display:block; }

body .cone, body .cone.route_elem_0, body.show_route .cone, .cone.active { fill:rgba(204,102,0, 0.5); display:block; }
body .cone_dir, body .cone_dir.route_elem_0, body.show_route .cone_dir, .cone_dir.active { fill:#009900; display:block; }

body .robot, body .robot.route_elem_0, body.show_route .robot, .robot.active { fill:#009900; display:block; }


text.info { fill:#FFFFFF; dominant-baseline:middle; text-anchor:middle; cursor:default; }
.info_r { fill:#337AB7; }
text.info.activ_over { fill:#FFFFFF !important; }
.info_r.activ_over { fill:#96D4D1 !important; stroke:none !important; }
.info_r.active { fill:#F0AD4E; }

.route_rayon.active { display:block; stroke:#000000; }

body .robot_secure, body .robot_secure.route_elem_0, body.show_route .robot_secure, .robot_secure.active { fill:rgba(0,0,0, 0.2); display:block; }
body.jobs_over .route_elem_num { fill:#F00; display:block; }

#map_tool .active { display:block !important; visibility:visible; }
#map_tool  .route_elem_num.active { display:none !important; }


body.show_rayon .cone, body.show_rayon .cone_dir, body.show_rayon .route, body.show_rayon .robot, body.show_rayon .robot_secure, body.show_rayon .poly , body.show_rayon .text, body.show_rayon .route_rayon { display:none; }
body.select_path .rayon { display:none; }

body.select .text, body .text.text_elem_0, .text.active { cursor:pointer; }
body.select .route, body .route.route_elem_0, body.show_route .route, .route.active { cursor:pointer; }
body.select .cone, body .cone.route_elem_0, body.show_route .cone, .cone.active { cursor:pointer; }
body.select .cone_dir,  body .cone_dir.route_elem_0, body.show_route .cone_dir, .cone_dir.active { cursor:pointer; }
body.select .robot, body .robot.route_elem_0, body.show_route .robot, .robot.active { cursor:pointer; }
body.select .robot_secure, body .robot_secure.route_elem_0, body.show_route .robot_secure, .robot_secure.active { cursor:pointer; }

body.select_path .robot_elem { cursor:pointer; }
.form-group { margin-bottom:10px; padding-top:5px; }
.form-group.active { background-color:#67A1B7; color:#FFFFFF; }
label.active { color:#F00; }

#masque { width:100%; height:100%; display:none; z-index:10001; position:absolute; left:0; top:0; }

#zoom_zone { z-index:1002; position:absolute; bottom:-50px; right:30px; width:300px; }
#zoom_zone .label { color:#000000; display:inline-block; font-weight:normal; margin-right:5px; margin-bottom:15px; }
#zoom_slider_contener { width:200px; display:inline-block; }
#zoom_slider-handle { width: 2em; height: 1.4em; top: 50%; margin-top: -.8em; text-align: center; line-height: 1.2em; font-size:0.6em; background-color:#FFFFFF; color:#67A1B7; border-color:#67A1B7; background-image:none; }

.legende_opacity { font-size:12px; }
#fond_opacite_slider { width:calc(100% - 60px); display:inline-block; }
#fond_opacite_slider-handle { width: 2em; height: 1.4em; top: 50%; margin-top: -.8em; text-align: center; line-height: 1.2em; font-size:0.6em; background-color:#FFFFFF; color:#67A1B7; border-color:#67A1B7; background-image:none; }

.fond_col_1 { display:inline-block; width:50px; font-size:0.9em; vertical-align:top; }
.fond_col_1.with_fond span { padding-top:23px; display:inline-block; }
.fond_col_2 { display:inline-block; width:calc(100% - 55px); border-left:1px solid #CCC; padding-left:5px; margin-bottom:10px; }


.ui-slider.ui-slider-horizontal { height: 0.5em; }

.ui-widget-content { background-color:#67A1B7; background-image:none; }

.movable { cursor:move !important; }

.todo_text { font-weight:bold; }

.job { border:1px solid #CCC; margin:10px 0; padding:10px; }
.job h3 { font-size:14px; line-height:30px; margin:0; padding:0; padding-left:10px; padding-right:10px; padding-bottom:10px; }
.job .actions { float:right; }
.job .actions .btn { margin-left:5px; }
.job .actions .btn:first-child { margin-left:0; }
.job li h3 { margin:0; padding:0; }

.ui-state-default { line-height:30px; }

#blocInfoOpened { background-color:#FFF; border-radius:0px; padding:10px; color:#000; position:absolute; top:0; left:0; overflow:auto; z-index:1003; cursor:pointer; }

.ui-state-default.multi_path { background-image:none; background-color:#EFEFEF;  }

.ui-state-default.multi_path .ui-state-default { background-image:none; background-color:#F5F5F5;  }

.sortable { list-style-type: none; margin: 0; padding: 0; width: 100%; }
.sortable li { margin: 0 3px 3px 3px; padding: 0.4em; padding-left: 1.5em; }
.sortable li .actions { white-space:nowrap; display:block; }
.sortable li .actions a, .sortable li .actions span { display:inline-block; }

.btn-group .badge { background-color:#5196CE; position:absolute; bottom:-10px; right:0px; top:auto; display:none; }

#infoHelp ul { list-style:none; margin:0; padding:0; }
#infoHelp ul span { min-width:140px; display:inline-block; }

.control-label span { font-size:0.8em; display:block; }

.tab-content { border:1px solid #ddd; border-top:0; padding:10px; padding-top:20px; }

#zoom_carte_container { z-index:1000; background-color:#FFF; position:absolute; top:0px; right:0px; }
#main_row.hide_element #zoom_carte_container { right:50px; }
#zoom_carte { position:relative; width:100px; border:1px solid #000; }

#main_row.hide_element #colonneCarte { padding-right:30px; }
#main_row.hide_element #colonneElements { display:none; }
#main_row #bToogleElements { font-size:24px; color:#333333; margin-right:20px; position:fixed; top:65px; right:-15px; }

#blocElements ul { margin:0; padding:0; list-style:none; }
#blocElements > ul > li { border-bottom:1px solid #CCC; margin-bottom:2px; }

#blocElements ul li.eye { cursor:pointer; }
#blocElements ul ul li.eye:hover > .head {  background-color:#EFEFEF; }

#blocElements ul ul li.eye.active > .head {  background-color:#E2EFFE; }

.no_icon_2 { display:inline-block; width:28px; }
.no_icon { display:inline-block; width:8px; }
.carre { display:inline-block; width:16px; }
.head { margin-bottom:1px; }

.eye { color:#333; }
.eye .fa-eye-slash { color:#999; }

.visibility_hide { visibility:hidden; }
body.show_rayon .rayon_elem.visibility_hide { visibility:visible; }
.activ_over.visibility_hide { visibility:visible; }
.activ_select.visibility_hide { visibility:visible; }

.selection { stroke:rgb(0, 0, 0); stroke-width: 1; fill:transparent; }



/* width */
::-webkit-scrollbar {
	height: 5px; 
  	width: 5px;
}

/* Track */
::-webkit-scrollbar-track {
  background: #f1f1f1; 
}
 
/* Handle */
::-webkit-scrollbar-thumb {
  background: #888; 
}

/* Handle on hover */
::-webkit-scrollbar-thumb:hover {
  background: #555; 
}

#avertCantChange { position:fixed; bottom:0; left:0; background-color:#C00; color:#FFFFFF; padding:2px 10px; opacity:0;}
#map_tool .alerte { animation: blink 2s linear infinite; }
@keyframes blink{
0%{opacity: 0;}
25%{opacity: 1;}
75%{opacity: 1;}
100%{opacity: 0;}
}

#map_tool #infos .btn { padding:6px 8px; }

#map_tool .btn-circle.btn-sm {
  width: 15px;
  height: 15px;
  text-align: center;
  padding: 1px 0 !important;
  font-size: 9px !important;
  border-radius: 7.5px;
}
#map_tool .btn-circle {
  width: 30px;
  height: 30px;
  text-align: center;
  padding: 6px 0;
  font-size: 12px;
  line-height: 1.428571429;
  border-radius: 15px;
}
#map_tool .btn-circle.btn-lg {
  width: 50px;
  height: 50px;
  padding: 10px 16px;
  font-size: 18px;
  line-height: 1.33;
  border-radius: 25px;
}
#map_tool .btn-circle.btn-xl {
  width: 70px;
  height: 70px;
  padding: 10px 16px;
  font-size: 24px;
  line-height: 1.33;
  border-radius: 35px;
}